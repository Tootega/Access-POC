{
  "compileOnSave": true,
  "noEmitOnError": false,
  "include": [
    "./**/*"
  ],
  "exclude": [
    "./**/*.d.ts",
    "./**/*.js"
  ],
  "compilerOptions": {
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "noImplicitReturns": true,
    "outFile": "../../../STX-Core/STX.Core.Service/wwwroot/static/js/STX.App.Core.API.js",
    "target": "es6",
    "module": "system",
    "strict": false,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "composite": true,
    "allowSyntheticDefaultImports": true
  }
}
